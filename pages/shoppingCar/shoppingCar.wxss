/* pages/shoppingCar/shoppingCar.wxss */
page{
  background-color:rgb(248, 248, 248);
  height:100%;
  width:100%;
}
.head{
  width:100%;
  display: flex;  
  border-top: 1px solid #F5F5F5;
  padding-top:10px;
  padding-bottom: 10px;
  background-color: #FFFFFF;
}
.deleteFn{
  font-size:16px;
  color:#666666;
  margin-right:10px;
  margin-left:auto;
}
.deleteFn.disabled{
  font-size:16px;
  color:#999999;
  margin-right:10px;
  margin-left:auto;
}
.postage{
  height:30px;
  width:100%;
  background-color:#FFD52E;
  font-size:12px;
  color:#2C2C2C;
  display: flex;  
  justify-content: center;  
  align-items: center;
  flex-direction: row  ;
}
.btn{
  background-color:#FFEEAA;
  width:31px;
  height:22px;
  font-size:11px;
  color:#333333;
  display: flex;  
  align-items: center;
  justify-content: center;  
}

.btn.btn_disabled{
  background-color:#D9D9D9;
  border:0;
  width:31px;
  height:22px;
  font-size:11px;
  color:#333333;
  display: flex;  
  align-items: center;
  justify-content: center;  
}


.list{
  display: flex;  
  align-items: center;
  flex-direction: row  ;
  height:94px;
  margin:0px 5px 5px 5px;
  border-radius: 4px;  
  background-color: #FFFFFF ;
 
}

.title{
  display: flex;  
  align-items: center;
  flex-direction: row  ;
  margin-top:0px;
}

.page_bottom{
  width:100%;
  height:55px;
  position:fixed; 
  bottom:0;
  border-top: 1px solid rgb(245, 245, 245);
  border-bottom: 1px solid rgb(245, 245, 245);
  background-color: #FFFFFF;
  display: flex;  
  flex-direction:row ;
  align-items: center;
}

.checkNum{
  width:116px;
  height:45px;
  background-color:#FFD52E;
  border-radius: 4px;
  display: flex;
  align-items: center;
  justify-content: center;  
  color:#333333;
  font-size:16px;
}