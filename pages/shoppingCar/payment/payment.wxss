/* pages/shoppingCar/payment/payment.wxss */
page{
  width:100%;
  height:100%;
  background-color: rgb(243, 243, 243);
}
.head{
  margin:10px 10px 10px 10px;
  height:50px;
  background-color: #FFFFFF;
  border:0.5px solid #FFD52E;
  border-radius: 4px;
  display: flex;
  align-items: center;
}
.head_address{
  margin:10px 10px 10px 10px;
  height:70px;
  background-color: #FFFFFF;
  border:0.5px solid #FFD52E;
  border-radius: 4px;
  display: flex;
  align-items: center;
}
.body{
  margin-left:10px;
  margin-right:10px;
  background-color: #FFFFFF;
  border-radius: 4px;
}

.page_bottom{
  width:100%;
  height:55px;
  position:fixed; 
  bottom:0;
  border-top: 1px solid #D9D9D9;
  background-color: #FFFFFF;
  display: flex;  
  align-items: center;
}

.toPayment{
  width:116px;
  height:45px;
  background-color:#FFD52E;
  border-radius: 4px;
  display: flex;
  align-items: center;
  justify-content: center;  
  color:#333333;
  font-size:16px;
}