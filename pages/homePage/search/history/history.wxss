/* pages/commodity/search/history/history.wxss */
.search{
 
  display: flex;  
  flex-direction: row  ;
  align-items: center;
  width:100%;
  height:30px;
 
}

.search_front {  
  border-radius: 110px;  
  margin-left: 10px;  
  margin-top:10px;
  margin-bottom: 10px;
  display: flex;  
  flex-direction: row  ;
  align-items: center;
  position: absolute;
  transition: all 1s;
  backface-visibility: hidden;
  cursor: pointer;
}  

 

.search_img_front{
  height:25px;
  width:25px;
  margin-top:10px;
  position: absolute;
  transition: all 1s;
  backface-visibility: hidden;
  cursor: pointer;
}

.search_back {  
 
  border-radius: 110px;  
  margin-left: 10px;  
  margin-top:10px;
  margin-bottom: 10px;
  display: flex;  
  flex-direction: row  ;
  align-items: center;
  position: absolute;
  transition: all 1s;
  backface-visibility: hidden;
  cursor: pointer;
} 

.search_img_back{
  height:25px;
  width:25px;
  margin-top:10px;
  position: absolute;
  transition: all 1s;
  backface-visibility: hidden;
  cursor: pointer;
}

.searchcion {  
  margin-left:10px;
  width: 15px;  
  height: 15px;  
}  

.cancel{
  color:rgb(92, 91, 91);
  font-size: 14px;
  margin-left:auto;
  margin-right:12px;
  align-items: center;
  color:#333333;
}

.title{
  margin-top:20px;
  display: flex;  
  flex-direction: row  ;
  align-items: center;
}

.history{
  display: flex;  
  flex-direction: row  ;
  align-items: center;
  flex-wrap: wrap;
  margin-top:10px;

}

.content{
  color:#666666;
  font-size:12px;
  background-color: rgb(245,245,245);
  box-sizing: border-box; 
  border-radius: 5px;  
  padding:5px 10px 5px 10px;
  margin-left:10px;
  margin-right:10px;
  margin-top:10px;
}